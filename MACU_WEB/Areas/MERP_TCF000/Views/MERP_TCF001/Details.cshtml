@model MACU_WEB.Areas.MERP_TCF000.ViewModels.MERP_TCF001_DetailsViewModel

@using MACU_WEB.Models;


@{
    string strPROG_ID = "MERP_TCF001";
    string strMENU_ID = "MERP_TCF000";
    ViewBag.Title = "Details";
    //ViewBag.Title = strPROG_ID.ToAutoMultiLang() + " - " + "PageQ".ToAutoMultiLang();
    Layout = "~/Views/Shared/_layout.cshtml";
}



<!-- BASIC FORM ELELEMNTS -->
<div class="row mt">
    <!-- col-lg-12全佔12行; main-chart 上邊空20px -->
    <div class="col-lg-12">
        <div class="form-panel">
            <h4 class="mb">
                <i class="fa fa-angle-right"></i> @strPROG_ID
                <i class="fa fa-angle-right"></i> @ViewBag.Title
            </h4>

            @using (Html.BeginForm("Details", strPROG_ID, FormMethod.Get, new { @class = "form-horizontal style-form" }))
            {
                <!-- form class="form-horizontal style-form" method="get" -->
                <div class="table-responsive  form-group">
                    <h2 class="col-sm-12 control-label">@Model.m_sMonth 月 @Model.m_sYear 年 勞健保</h2>
                    <div class="col-sm-12">
                        <table class="table table-hover rwd-table" id="M_TCC_TAB_1">
                            <thead>
                                <tr>
                                    <!-- Total 39行 Cols + IsValid,createTime,updateTime -->
                                    <th class="col-sm-1 "> 編號</th>
                                    <th class="col-sm-1 "> 部門</th>
                                    <th class="col-sm-1 "> 加保公司</th>
                                    <th class="col-sm-1 "> 編碼</th>
                                    <th class="col-sm-1 "> 勞保</th>
                                    <th class="col-sm-1 "> 健保</th>
                                    <th class="col-sm-1 "> 團保</th>
                                    <th class="col-sm-1 "> 職稱</th>
                                    <th class="col-sm-1 "> 到職日</th>
                                    <th class="col-sm-1 "> 離職日</th>
                                    <th class="col-sm-1 "> 年資</th>
                                    <th class="col-sm-1 "> 保密</th>
                                    <th class="col-sm-1 "> 薪資</th>
                                    <th class="col-sm-1 "> 姓名</th>
                                    <th class="col-sm-1 "> 資料年份</th>
                                    <th class="col-sm-1 "> 資料月份</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{int i = 0;}
                                @foreach (var item in Model.m_oFALaborHealthInsList)
                                {
                                    <tr>
                                        <td>
                                            @item.Id
                                        </td>
                                        <td>
                                            @item.DepartName
                                        </td>
                                        <td>
                                            @item.PlusInsCompany
                                        </td>
                                        <td>
                                            @item.Coding
                                        </td>
                                        <td>
                                            @item.LaborIns
                                        </td>
                                        <td>
                                            @item.HealthIns
                                        </td>
                                        <td>
                                            @item.GroupIns
                                        </td>
                                        <td>
                                            @item.JobTitle
                                        </td>
                                        <td>
                                            @item.OnJobDate
                                        </td>
                                        <td>
                                            @item.ResignDate
                                        </td>
                                        <td>
                                            @item.Seniority
                                        </td>
                                        <td>
                                            @item.KeepSecret
                                        </td>
                                        <td>
                                            @item.Salary
                                        </td>
                                        <td>
                                            @item.MemberName
                                        </td>
                                        <td>
                                            @item.DataYear
                                        </td>
                                        <td>
                                            @item.DataMonth
                                        </td>

                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>

                <!-- /form -->
            }
        </div>

    </div><!-- /col-lg-12 -->

</div><!-- /row -->



